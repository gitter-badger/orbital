{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "ordered-imports": false,
    "interface-name": false,
    "member-access": false,
    "member-ordering": false,
    "array-type": false,
    "arrow-parens": false
  },
  "cliOptions": {
    "exclude": [
      "test"
    ]
  }
}
